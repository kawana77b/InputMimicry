{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/**/InputMimicry.csproj"],
          "src": "../"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**", "favicon.ico", "logo.svg"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appName": "InputMimicry",
      "_appTitle": "InputMimicry Documentation",
      "_enableSearch": true,
      "_disableAffix": true,
      "_enableNewTab": true,
      "_appFooter": "Generated by <b>DocFX</b>",
      "_disableSideFilter": false,
      "_gitContribute": {
        "repo": "https://github.com/kawana77b/InputMimicry"
      },
      "_gitUrlPattern": "github"
    },
    "fileMetadataFiles": [],
    "template": ["default", "templates/discordfx"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
